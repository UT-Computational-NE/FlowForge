4
(
    inlet
    {
        type            patch;
        nFaces          5;
        startFace       895;
    }
    outlet
    {
        type            patch;
        nFaces          5;
        startFace       900;
    }
    walls
    {
        type            wall;
        inGroups        1(wall);
        nFaces          200;
        startFace       905;
    }
    defaultFaces
    {
        type            empty;
        inGroups        1(empty);
